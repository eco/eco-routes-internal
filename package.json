{
  "name": "@eco-foundation/routes",
  "version": "1.8.14",
  "description": "The Eco Routes Protocol is a decentralized intent based system that allows users to submit their intent to the network and have it fulfilled by a solver on the destination rollup of their choise.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=v22.10.0"
  },
  "homepage": "https://github.com/eco/eco-routes#readme",
  "bugs": {
    "url": "https://github.com/eco/eco-routes/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eco/eco-routes.git"
  },
  "author": "Eco Engineering <eng@eco.com>",
  "license": "MIT",
  "keywords": [
    "solidity",
    "ethereum",
    "eco routes",
    "routes",
    "bend",
    "protocol",
    "rollup",
    "solver"
  ],
  "scripts": {
    "format": "concurrently 'npm:format:*'",
    "format:eslint": "eslint --fix --ext .js,.ts,.tsx .",
    "format:prettier": "prettier --write .",
    "format:solhint": "solhint --fix --noPrompt contracts/**/*.sol",
    "depcheck": "depcheck --ignore-bin-package --ignores @openzeppelin/contracts,@openzeppelin/contracts-upgradeable,prettier-plugin-solidity,solhint-plugin-eco,concurrently,@types/mocha",
    "lint": "yarn format",
    "build": "hardhat compile",
    "test": "yarn test:hardhat && yarn test:ts",
    "test:hardhat": "hardhat test",
    "test:ts": "jest",
    "clean": "hardhat clean && yarn pub:clean",
    "docgen": "hardhat docgen",
    "testGas": "ENABLE_GAS_REPORT=1 hardhat test",
    "testParallel": "hardhat test --parallel",
    "snapshots": "UPDATE_SNAPSHOT=1 hardhat test",
    "debug": "hardhat test",
    "coverage": "SOLIDITY_COVERAGE=1 hardhat coverage",
    "coverageReport": "SOLIDITY_COVERAGE=1 hardhat coverage; cd coverage; python3 -m http.server 7800",
    "callGraph": "slither . --print call-graph; dot all_contracts.call-graph.dot -Tpng -o ./docs/slither/call-graph.png; rm *.call-graph.dot",
    "inheritanceGraph": "slither . --print inheritance-graph; dot inheritance-graph.dot -Tpng -o ./docs/slither/inheritance-graph.png; rm inheritance-graph.dot",
    "variableOrder": "slither --print variable-order . 2>&1 | sed -r 's/\\x1B\\[([0-9]{1,3}(;[0-9]{1,2};?)?)?[mGK]//g' | tee  ./docs/slither/variable-order.txt",
    "vulnerability": "slither . --exclude-informational --exclude-low --exclude-medium 2>&1 | sed -r 's/\\x1B\\[([0-9]{1,3}(;[0-9]{1,2};?)?)?[mGK]//g' | tee  ./docs/slither/vulnerability.txt",
    "contractSummary": "slither --print contract-summary . 2>&1 | tee ./docs/slither/contract-summary.txt",
    "Testnet": "-------- Testnet scripts below",
    "deployEcoTestnet": "hardhat run --network ecoTestnet scripts/testnet/deploy.ts",
    "deployBaseSepolia": "hardhat run --network baseSepolia scripts/testnet/deploy.ts",
    "deployOptimismSepolia": "hardhat run --network optimismSepoliaBlockscout scripts/testnet/deploy.ts",
    "deployArbitrumSepolia": "hardhat run --network arbitrumSepolia scripts/testnet/deploy.ts",
    "deployMantleSepolia": "hardhat run --network mantleSepolia scripts/testnet/deploy.ts",
    "deployPolygonSepolia": "hardhat run --network polygonSepolia scripts/testnet/deploy.ts",
    "deployTestnet": "yarn deployEcoTestnet &&  yarn deployBaseSepolia && yarn deployOptimismSepolia && yarn deployMantleSepolia && yarn deployArbitrumSepolia & yarn deployArbitrumSepolia",
    "proverDeployEcoTestnet": "hardhat run --network ecoTestnet scripts/testnet/proverDeploy.ts",
    "proverDeployBaseSepolia": "hardhat run --network baseSepolia scripts/testnet/proverDeploy.ts",
    "proverDeployOptimismSepolia": "hardhat run --network optimismSepoliaBlockscout scripts/testnet/proverDeploy.ts",
    "intentSolve": "hardhat run scripts/testnet/intentSolve.ts",
    "intentWithdraw": "hardhat run scripts/testnet/intentWithdraw.ts",
    "TestnetBedrock": "-------- Bedrock scripts below --------",
    "TestnetCannon": "-------- Cannon scripts below --------",
    "intentWithdrawCannon": "hardhat run scripts/testnet/cannon/intentWithdraw.ts",
    "Mainnet": "-------- Mainnet scripts below",
    "proverDeployBase": "hardhat run --network base scripts/mainnet/proverDeploy.ts",
    "proverDeployOptimism": "hardhat run --network optimism scripts/mainnet/proverDeploy.ts",
    "deployBase": "hardhat run --network base scripts/mainnet/deploy.ts",
    "deployBaseOpen": "hardhat run --network base scripts/mainnet/deployOpen.ts",
    "deployOptimism": "hardhat run --network optimism scripts/mainnet/deploy.ts",
    "deployOptimismOpen": "hardhat run --network optimism scripts/mainnet/deployOpen.ts",
    "deployArbitrum": "hardhat run --network arbitrum scripts/mainnet/deploy.ts",
    "deployMantle": "hardhat run --network mantle scripts/mainnet/deploy.ts",
    "deployPolygon": "hardhat run --network polygon scripts/mainnet/deploy.ts",
    "deployHyperProverBase": "hardhat run --network base scripts/deployHyperProver.ts",
    "deployHyperProverOptimism": "hardhat run --network optimism scripts/deployHyperProver.ts",
    "MainnetBedrock": "-------- Bedrock scripts below --------",
    "intentSolveMainnet": "hardhat run scripts/mainnet/intentSolve.ts",
    "intentWithdrawOpBase": "hardhat run scripts/mainnet/intentWithdrawOpBase.ts",
    "intentWithdrawBaseOp": "hardhat run scripts/mainnet/intentWithdrawBaseOp.ts",
    "MainnetCannon": "-------- Cannon scripts below --------",
    "EndMainnetCannon": "-------- End Cannon scripts  --------",
    "versionPackage": "npx tsx scripts/publish/version.ts",
    "prepack": "rimraf build && rimraf buildTs && yarn run versionPackage && bash scripts/publish/prepack.sh",
    "deployCI": "npx tsx scripts/deploy/deployCI.ts",
    "deployForgeCI": "npx tsx scripts/deploy/deployForgeCI.ts",
    "pub:clean": "rimraf build/dist",
    "pub:build": "cd build && tsc",
    "pub:prepack": "yarn pub:clean && yarn pub:build",
    "pub:tag": "npx tsx scripts/publish/tag.ts",
    "viemDeployCI": "npx tsx scripts/viem_deploy/deploy.ts",
    "pub:publish": "npx tsx scripts/publish/publish.ts"
  },
  "devDependencies": {
    "@actions/core": "^1.11.1",
    "@alchemy/aa-core": "^3.19.0",
    "@eth-optimism/contracts-bedrock": "^0.17.2",
    "@hyperlane-xyz/registry": "^4.2.0",
    "@hyperlane-xyz/sdk": "^5.1.0",
    "@nomicfoundation/hardhat-chai-matchers": "^2.0.2",
    "@nomicfoundation/hardhat-ethers": "^3.0.5",
    "@nomicfoundation/hardhat-network-helpers": "^1.0.10",
    "@nomicfoundation/hardhat-toolbox": "^4.0.0",
    "@nomicfoundation/hardhat-verify": "^2.0.12",
    "@nomicfoundation/hardhat-viem": "^1.0.2",
    "@nomiclabs/hardhat-ethers": "^2.2.3",
    "@nomiclabs/hardhat-etherscan": "^3.1.8",
    "@nomiclabs/hardhat-waffle": "^2.0.6",
    "@openzeppelin/contracts": "^5.0.2",
    "@openzeppelin/contracts-upgradeable": "^5.0.2",
    "@openzeppelin/hardhat-upgrades": "^3.2.0",
    "@typechain/ethers-v6": "^0.5.1",
    "@typechain/hardhat": "^9.1.0",
    "@types/chai": "^4.2.0",
    "@types/jest": "^29.5.14",
    "@types/lodash": "^4.17.13",
    "@types/mocha": ">=9.1.0",
    "@types/node": "^22.9.0",
    "@types/pacote": "^11.1.8",
    "@types/semver-utils": "^1.1.3",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@wagmi/cli": "^2.0.0-rc.3",
    "chai": "^4.3.10",
    "concurrently": "^8.2.2",
    "depcheck": "^1.4.7",
    "dotenv": "^16.3.1",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-chai": "^0.0.1",
    "eslint-plugin-chai-friendly": "^0.7.2",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-mocha": "^10.2.0",
    "eslint-plugin-n": "^16.4.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-promise": "^6.1.1",
    "ethers": "^6.9.0",
    "hardhat": "^2.22.6",
    "hardhat-gas-reporter": "^1.0.9",
    "jest": "^29.7.0",
    "lodash": "^4.17.21",
    "p-queue": "^8.0.1",
    "pacote": "^21.0.0",
    "prettier": "^3.1.1",
    "prettier-plugin-solidity": "^1.4.1",
    "rimraf": "^6.0.1",
    "semver-utils": "^1.1.4",
    "solhint": "^4.0.0",
    "solhint-plugin-eco": "^0.1.0",
    "solidity-coverage": "^0.8.5",
    "solidity-docgen": "^0.6.0-beta.36",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.2",
    "typechain": "^8.3.2",
    "typescript": ">=4.5.0"
  },
  "dependencies": {
    "@axelar-network/axelar-gmp-sdk-solidity": "^6.0.4",
    "@hyperlane-xyz/registry": "^4.2.0",
    "@hyperlane-xyz/sdk": "^5.1.0",
    "@openzeppelin/contracts": "^5.0.2",
    "@openzeppelin/contracts-upgradeable": "^5.0.2",
    "viem": "^2.22.21"
  }
}
